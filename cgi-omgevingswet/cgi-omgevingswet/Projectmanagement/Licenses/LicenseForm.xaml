<Window x:Class="cgi_omgevingswet.Projectmanagement.Licenses.LicenseForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LicenseForm" Height="600" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        
        <Label FontSize="32" Content="Vergunningsaanvraag voor&#10;[projectnaam]" />
        
        <Grid Grid.Row="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="200" />
                <RowDefinition Height="100" />
            </Grid.RowDefinitions>
            
            <Label Content="Vergunningsnaam: " />
        </Grid>
    </Grid>
</Window>
